{
	"name": "ts-nameof-proxy",
	"version": "0.1.14514",
	"description": "`nameof` in TypeScript, no compiler required. Implemented using proxy with some limitations.",
	"keywords": [
		"nameof",
		"formik"
	],
	"author": "pinkchampagne",
	"homepage": "https://github.com/p-toy-factory/ts-nameof-proxy",
	"license": "MIT",
	"scripts": {
		"build": "rimraf ./dist && rollup --config",
		"lint": "eslint src",
		"lint:fix": "npm run lint -- --fix",
		"test": "jest"
	},
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist/**/*.*"
	],
	"devDependencies": {
		"@babel/core": "^7.17.10",
		"@babel/preset-env": "^7.17.10",
		"@babel/preset-typescript": "^7.16.7",
		"@rollup/plugin-typescript": "^8.5.0",
		"@types/jest": "^27.5.1",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"babel-jest": "^28.1.0",
		"eslint": "^8.48.0",
		"eslint-config-tnp": "workspace:*",
		"jest": "^28.1.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.79.0",
		"rollup-plugin-dts": "^5.3.0",
		"rollup-plugin-terser": "^7.0.2",
		"ts-jest": "^28.0.2",
		"tslib": "^2.4.0",
		"typescript": "^4.6.4"
	}
}
